<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Parallel Testing Suite" thread-count="3">
    <test name="Firefox testing">
        <parameter name="browser-type" value="firefox"></parameter>
        <classes>
            <class name="com.ceng.webonline.WebOnlineTestFactory"></class>
        </classes>
    </test>

    <test name="Chrome testing">
        <parameter name="browser-type" value="chrome"></parameter>
        <classes>
            <class name="com.ceng.webonline.WebOnlineTestFactory"></class>
        </classes>
    </test>
</suite>